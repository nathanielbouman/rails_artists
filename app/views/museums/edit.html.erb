<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="museums edit">
  <h1>Edit Museum</h1>

  <%= form_for(:museum, :url => {:action => 'update', :id => @museum.id}) do |f| %>

    <table summary="Museum form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
    </table>

    <div class="form-buttons">
      <%= submit_tag("Update Museum") %>
    </div>

  <% end %>
</div>

